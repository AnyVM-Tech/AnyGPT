{
	"name": "anygpt-api",
	"version": "1.0.0",
	"packageManager": "pnpm@10.5.2",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"build": "pnpm clean && tsc -p tsconfig.json --outDir ./dist && node setup.js",
		"build:tsgo": "pnpm clean && tsgo --project tsconfig.json --outDir ./dist --noEmit false && node setup.js",
		"start": "node ./dist/server.js",
		"dev": "npx tsx server.ts",
		"lint": "eslint . --ext .ts",
		"typecheck": "tsgo --noEmit",
		"__comment_test__": "The main test script below uses concurrently to run the mock provider, server and the test runner",
		"test": "concurrently --kill-others --success first --names \"MOCK,API,TEST\" -c \"bgYellow.bold,bgBlue.bold,bgMagenta.bold\" \"pnpm:test:mock\" \"pnpm:test:dev\" \"wait-on tcp:localhost:3001 tcp:localhost:3000 && pnpm test:run\"",
		"test:mock": "cross-env NODE_ENV=test tsx ./dev/mockProvider.ts",
		"test:dev": "cross-env NODE_ENV=test DATA_SOURCE_PREFERENCE=filesystem tsx server.ts",
		"test:run": "cross-env NODE_ENV=test tsx ./dev/testApi.ts",
		"test:ws": "cross-env NODE_ENV=test tsx ./dev/testWs.ts"
	},
	"dependencies": {
		"@google/generative-ai": "^0.11.3",
		"@types/ajv": "^1.0.4",
		"@types/express": "^4.17.21",
		"ajv": "^8.17.1",
		"axios": "^1.7.2",
		"chalk": "^5.3.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"eventsource": "^2.0.2",
		"express": "^4.19.2",
		"hyper-express": "^6.16.3",
		"ioredis": "^5.5.0",
		"openai": "^4.47.1",
		"selenium-webdriver": "^4.26.0",
		"uuid": "^11.1.0",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.7",
		"@types/ws": "^8.18.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.2.0",
		"jest": "^27.2.5",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.5",
		"ts-node-dev": "^1.1.8",
		"tsx": "^4.7.1",
		"typescript": "^5.4.5",
		"wait-on": "^8.0.3"
	}
}
