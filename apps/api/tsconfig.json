{
	"compilerOptions": {
	  "target": "ES2022",
	  "module": "ESNext",
	  "outDir": "./dist",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "paths": { 
		"*": ["./*", "./node_modules/*"]
	  },
	  "moduleResolution": "bundler", 
	  "resolveJsonModule": true, 
	  "allowSyntheticDefaultImports": true,
	  "noEmit": false,
	  "declaration": true,
	  "emitDeclarationOnly": false,
	  "noEmitOnError": false
	},
	"include": ["./**/*.ts", "./**/*.d.ts"],
	"exclude": [
		"node_modules",
		"dist",
		"logs",
		"providers.json",
		"keys.json",
		"models.json",
		"providers.schema.json",
		"models.schema.json",
		"tiers.json",
		"dev/tps.ts"
	]
  }