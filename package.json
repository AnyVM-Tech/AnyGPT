{
	"name": "anygpt",
	"version": "1.0.0",
	"private": true,
	"description": "AnyGPT, the only free API and apps you'll ever need",
	"workspaces": [
		"apps/*"
	],
	"scripts": {
		"build": "tsc -b",
		"dev": "turbo run dev",
		"test": "turbo run test",
		"lint": "turbo run lint",
		"typecheck": "tsc --noEmit",
		"format": "prettier --write ./",
		"prepare": "husky",
		"setup": "pnpm --filter anygpt-api run setup"
	},
	"devDependencies": {
		"eslint": "^8.2.0",
		"husky": "^9.1.7",
		"prettier": "^3.3.2",
		"turbo": "^2.0.4",
		"typescript": "^5.4.5"
	},
	"packageManager": "pnpm@10.29.2",
	"dependencies": {
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"pnpm": "^10.28.2"
	}
}
